{
  "name": "ecommerce",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite preview",
    "prettier": "prettier --check --ignore-unknown src/",
    "prettier:fix": "prettier --write --ignore-unknown src/",
    "formatAll": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky",
    "types-check": "tsc --noEmit",
    "pre-push": "npm run lint && npm run prettier && npm run types-check && npm run test",
    "test": "jest"
  },
  "dependencies": {
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "sass": "^1.76.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.2.0"
  }
}
